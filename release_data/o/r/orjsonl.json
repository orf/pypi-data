    "0.1.3": {
        "info": {
            "author": "",
            "author_email": "Umar Butler <umar@umar.au>",
            "bugtrack_url": null,
            "classifiers": [
                "Development Status :: 5 - Production/Stable",
                "Intended Audience :: Developers",
