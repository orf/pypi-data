    "2.1.0": {
        "info": {
            "author": "Farama Foundation",
            "author_email": "contact@farama.org",
            "bugtrack_url": null,
            "classifiers": [
                "Development Status :: 5 - Production/Stable",
                "Programming Language :: Python :: 3",
