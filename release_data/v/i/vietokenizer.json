    "1.0.3": {
        "info": {
            "author": "Dat Tien Nguyen",
            "author_email": "nduc0231@gmail.com",
            "bugtrack_url": null,
            "classifiers": [
                "Development Status :: 3 - Alpha",
                "Intended Audience :: Developers",
