        "info": {
            "author": "Christoph Krybus",
            "author_email": "chris@ckrybus.com",
            "bugtrack_url": null,
            "classifiers": [
                "Development Status :: 4 - Beta",
                "Environment :: Web Environment",
                "Framework :: Django",
