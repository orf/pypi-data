    "0.4.19": {
        "info": {
            "author": "Steve @siznax",
            "author_email": "steve@siznax.net",
            "bugtrack_url": null,
            "classifiers": [
                "Development Status :: 3 - Alpha",
                "Intended Audience :: Developers",
