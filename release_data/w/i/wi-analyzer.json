    "0.1.2": {
        "info": {
            "author": "",
            "author_email": "Eyal Zekaria <eyal.z@doit-intl.com>",
            "bugtrack_url": null,
            "classifiers": [
                "Programming Language :: Python :: 3 :: Only"
