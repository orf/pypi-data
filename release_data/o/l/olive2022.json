    "0.1.4": {
        "info": {
            "author": "Carnegie Mellon University",
            "author_email": "satya+group@cs.cmu.edu",
            "bugtrack_url": null,
            "classifiers": [
                "Development Status :: 4 - Beta",
                "Environment :: Console",
