        "info": {
            "author": "XuMing",
            "author_email": "xuming624@qq.com",
            "bugtrack_url": null,
            "classifiers": [
                "Intended Audience :: Developers",
                "Natural Language :: Chinese (Simplified)",
                "Natural Language :: Chinese (Traditional)",
                "Operating System :: OS Independent",
