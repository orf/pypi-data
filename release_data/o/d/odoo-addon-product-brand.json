    "16.0.1.0.0.2": {
        "info": {
            "author": "NetAndCo, Akretion, Prisnet Telecommunications SA, MONK Software, SerpentCS Pvt. Ltd., Tecnativa, Kaushal Prajapati, Odoo Community Association (OCA)",
            "author_email": "support@odoo-community.org",
            "bugtrack_url": null,
            "classifiers": [
