    "0.0.6": {
        "info": {
            "author": "Ora",
            "author_email": "victor.xsyang@gmail.com",
            "bugtrack_url": null,
            "classifiers": [
                "License :: OSI Approved :: MIT License",
                "Operating System :: OS Independent",
