    "1.0.2": {
        "info": {
            "author": null,
            "author_email": "\"ANSYS, Inc.\" <pyansys.support@ansys.com>",
            "bugtrack_url": null,
            "classifiers": [
                "Development Status :: 4 - Beta",
                "License :: OSI Approved :: MIT License",
