    "0.0.3": {
        "info": {
            "author": "Nasajon Sistemas",
            "author_email": "contact.dev@nasajon.com.br",
            "bugtrack_url": null,
            "classifiers": [
                "Development Status :: 3 - Alpha",
                "Intended Audience :: Developers",
