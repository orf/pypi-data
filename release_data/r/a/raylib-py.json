    "4.2.0.post1": {
        "info": {
            "author": "",
            "author_email": "\"Jorge A. Gomes\" <jorgegomes83@protonmail.com>",
            "bugtrack_url": null,
            "classifiers": [
                "License :: OSI Approved :: MIT License",
                "Operating System :: OS Independent",
                "Programming Language :: Python :: 3"
