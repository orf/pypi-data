    "0.1.4": {
        "info": {
            "author": "JerBouma",
            "author_email": "jer.bouma@gmail.com",
            "bugtrack_url": null,
            "classifiers": [
                "Development Status :: 5 - Production/Stable",
                "Intended Audience :: Developers",
