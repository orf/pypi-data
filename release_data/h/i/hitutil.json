    "0.5.0": {
        "info": {
            "author": "Billchenchina",
            "author_email": "billchenchina2001@gmail.com",
            "bugtrack_url": null,
            "classifiers": [
                "Development Status :: 2 - Pre-Alpha",
                "Intended Audience :: Developers",
