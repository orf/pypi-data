    "0.4.0": {
        "info": {
            "author": "",
            "author_email": "Oleg Kostromin <kostromin97@gmail.com>, Iryna Kondrashchenko <iryna230520@gmail.com>, Marco Pasini <marco.pasini.98@gmail.com>",
            "bugtrack_url": null,
