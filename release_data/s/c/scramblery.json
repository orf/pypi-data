    "1.2": {
        "info": {
            "author": "altunenes",
            "author_email": "enesaltun2@gmail.com",
            "bugtrack_url": null,
            "classifiers": [
                "License :: OSI Approved :: MIT License",
                "Operating System :: OS Independent",
