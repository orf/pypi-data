    "0.3.2": {
        "info": {
            "author": "Krokotsch, Tilman",
            "author_email": "tilman.krokotsch@tu-berlin.de",
            "bugtrack_url": null,
            "classifiers": [
                "License :: OSI Approved :: MIT License",
