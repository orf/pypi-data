    "1.0.3": {
        "info": {
            "author": "DVAdmin",
            "author_email": "liqiang@django-vue-admin.com",
            "bugtrack_url": null,
            "classifiers": [
                "License :: OSI Approved :: MIT License",
                "Operating System :: OS Independent",
