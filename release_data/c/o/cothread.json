    "2.18.1": {
        "info": {
            "author": "Michael Abbott",
            "author_email": "Michael.Abbott@diamond.ac.uk",
            "bugtrack_url": null,
            "classifiers": [
                "Development Status :: 5 - Production/Stable",
                "Intended Audience :: Developers",
