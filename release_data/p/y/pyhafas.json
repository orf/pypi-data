    "0.3.0": {
        "info": {
            "author": "Simeon Keske, Leo Maroni",
            "author_email": "dev@n0emis.eu, hello@em0lar.de",
            "bugtrack_url": null,
            "classifiers": [
                "Development Status :: 4 - Beta",
                "Intended Audience :: Developers",
                "License :: OSI Approved :: MIT License",
