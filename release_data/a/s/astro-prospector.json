    "1.1.0": {
        "info": {
            "author": "Ben Johnson",
            "author_email": "benjamin.johnson@cfa.harvard.edu",
            "bugtrack_url": null,
            "classifiers": [
                "Development Status :: 5 - Production/Stable",
                "Intended Audience :: Science/Research",
