    "2.0.0a2": {
        "info": {
            "author": "Dylan Anthony",
            "author_email": "danthony@triaxtec.com",
            "bugtrack_url": null,
            "classifiers": [
                "Development Status :: 5 - Production/Stable",
                "Intended Audience :: Developers",
