    "0.1.2": {
        "info": {
            "author": "Themis AI",
            "author_email": "info@themisai.io",
            "bugtrack_url": null,
            "classifiers": [
                "Development Status :: 2 - Pre-Alpha",
                "Intended Audience :: Developers",
