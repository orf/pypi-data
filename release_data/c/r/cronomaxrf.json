    "0.0.8": {
        "info": {
            "author": "Frank Ligterink",
            "author_email": "frank.ligterink@gmail.com",
            "bugtrack_url": null,
            "classifiers": [
                "Development Status :: 3 - Alpha",
                "Intended Audience :: Developers",
