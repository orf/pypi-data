    "0.3.0": {
        "info": {
            "author": "Charles Brunet",
            "author_email": "charles@cbrunet.net",
            "bugtrack_url": null,
            "classifiers": [
                "Development Status :: 4 - Beta",
                "Intended Audience :: Developers",
                "License :: OSI Approved :: GNU General Public License v2 (GPLv2)",
                "Programming Language :: Python :: 3.10",
