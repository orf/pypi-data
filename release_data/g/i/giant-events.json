    "0.2.6": {
        "info": {
            "author": "Will-Hoey",
            "author_email": "will.hoey@giantmade.com",
            "bugtrack_url": null,
            "classifiers": [
                "Environment :: Console",
                "License :: OSI Approved :: MIT License",
                "Operating System :: OS Independent",
                "Programming Language :: Python :: 3",
