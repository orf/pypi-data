    "1.2.0": {
        "info": {
            "author": "Jacob A. Thompson",
            "author_email": "jacobalthompson@gmail.com",
            "bugtrack_url": null,
            "classifiers": [
                "Development Status :: 5 - Production/Stable",
                "License :: OSI Approved :: MIT License",
