    "0.1.2": {
        "info": {
            "author": "Archie Harrison",
            "author_email": "aj.harrison04@gmail.com",
            "bugtrack_url": null,
            "classifiers": [
                "Development Status :: 3 - Alpha",
                "Intended Audience :: Developers",
