    "1.0.3": {
        "info": {
            "author": "principle105",
            "author_email": "principle105@gmail.com",
            "bugtrack_url": null,
            "classifiers": [
                "License :: OSI Approved :: MIT License",
                "Programming Language :: Python :: 3",
