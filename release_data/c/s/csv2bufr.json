    "0.3.1": {
        "info": {
            "author": "David I. Berry",
            "author_email": "DBerry@wmo.int",
            "bugtrack_url": null,
            "classifiers": [
                "Development Status :: 3 - Alpha",
                "Environment :: Console",
                "Intended Audience :: Developers",
