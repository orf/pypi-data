    "2.0.3": {
        "info": {
            "author": "Open Data Team",
            "author_email": "contact@opendata.team",
            "bugtrack_url": null,
            "classifiers": [
                "Development Status :: 3 - Alpha",
                "Environment :: Web Environment",
                "Intended Audience :: Developers",
                "Operating System :: OS Independent",
                "Programming Language :: Python",
