    "1.0.3": {
        "info": {
            "author": "He xi",
            "author_email": "xmhexi@qq.com",
            "bugtrack_url": null,
            "classifiers": [
                "Development Status :: 5 - Production/Stable",
                "Intended Audience :: Developers",
