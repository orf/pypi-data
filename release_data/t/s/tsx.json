    "0.0.4": {
        "info": {
            "author": "Andrei Suiu",
            "author_email": "andrei.suiu@gmail.com",
            "bugtrack_url": null,
            "classifiers": [
                "Development Status :: 5 - Production/Stable",
                "Intended Audience :: Developers",
