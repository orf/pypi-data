    "0.6.0": {
        "info": {
            "author": null,
            "author_email": "Roxanne El Baff <roxanne.elbaff@dlr.de>, Tobias Hecking <tobias.hecking@dlr.de>",
            "bugtrack_url": null,
            "classifiers": [
