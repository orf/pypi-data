        "info": {
            "author": "Mart\u00ed Bosch",
            "author_email": "marti.bosch@epfl.ch",
            "bugtrack_url": null,
            "classifiers": [
                "License :: OSI Approved :: GNU Lesser General Public License v3 (LGPLv3)",
                "Programming Language :: Python",
                "Programming Language :: Python :: 3",
