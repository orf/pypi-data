    "0.3.0": {
        "info": {
            "author": "Timmy Welch",
            "author_email": "timmy@narnian.us",
            "bugtrack_url": null,
            "classifiers": [
                "License :: OSI Approved :: MIT License",
                "Programming Language :: Python :: 3",
