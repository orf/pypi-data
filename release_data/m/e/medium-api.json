    "0.2.8": {
        "info": {
            "author": "Nishu Jain",
            "author_email": "nishujain1997.19@gmail.com",
            "bugtrack_url": null,
            "classifiers": [
                "Development Status :: 5 - Production/Stable",
                "Intended Audience :: Developers",
