    "0.1.2": {
        "info": {
            "author": "Lemuel Boyce",
            "author_email": "lemuelboyce@gmail.com",
            "bugtrack_url": null,
            "classifiers": [
                "Development Status :: 2 - Pre-Alpha",
                "Environment :: Console",
