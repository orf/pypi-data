    "0.0.52": {
        "info": {
            "author": "Jack Liu",
            "author_email": "\"David C. Heson\" <dch376@msstate.edu>, William Robertson <william.robertson@mtsu.edu>",
            "bugtrack_url": null,
            "classifiers": [
