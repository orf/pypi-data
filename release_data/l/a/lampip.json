    "0.2.0": {
        "info": {
            "author": "hayashiya18",
            "author_email": "sei8haya@gmail.com",
            "bugtrack_url": null,
            "classifiers": [
                "License :: OSI Approved :: MIT License",
                "Programming Language :: Python :: 3",
