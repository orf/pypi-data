    "0.2.0": {
        "info": {
            "author": "yanyongyu",
            "author_email": "yyy@nonebot.dev",
            "bugtrack_url": null,
            "classifiers": [
                "License :: OSI Approved :: MIT License",
                "Programming Language :: Python :: 3",
