    "0.1.2": {
        "info": {
            "author": "Sadahiro YOSHIKAWA",
            "author_email": "",
            "bugtrack_url": null,
            "classifiers": [
                "License :: OSI Approved :: MIT License",
                "Programming Language :: Python :: 3",
