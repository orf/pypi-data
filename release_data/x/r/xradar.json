    "0.0.8": {
        "info": {
            "author": "['Maxwell Grover, Kai Muehlbauer, Zachary Sherman']",
            "author_email": "mgrover@anl.gov",
            "bugtrack_url": null,
            "classifiers": [
                "Development Status :: 2 - Pre-Alpha",
                "Intended Audience :: Developers",
                "License :: OSI Approved :: MIT License",
                "Natural Language :: English",
