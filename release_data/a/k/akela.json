    "0.3": {
        "info": {
            "author": "sherekhan at pypi.org",
            "author_email": "sherekhan@jungle.tes",
            "bugtrack_url": null,
            "classifiers": [
                "Development Status :: 2 - Pre-Alpha",
                "License :: OSI Approved :: GNU General Public License v3 or later (GPLv3+)",
