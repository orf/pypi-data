    "0.1.1": {
        "info": {
            "author": "pwwang",
            "author_email": "pwwang@pwwang.com",
            "bugtrack_url": null,
            "classifiers": [
                "License :: OSI Approved :: MIT License",
                "Programming Language :: Python :: 3",
