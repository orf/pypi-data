    "3.5.0": {
        "info": {
            "author": "Guilhem Saurel",
            "author_email": "guilhem.saurel@laas.fr",
            "bugtrack_url": null,
            "classifiers": [
                "License :: OSI Approved :: BSD License",
                "Programming Language :: Python :: 3",
