    "0.2.2": {
        "info": {
            "author": "Bram Daams",
            "author_email": "b.daams@science.ru.nl",
            "bugtrack_url": null,
            "classifiers": [
                "Development Status :: 2 - Pre-Alpha",
                "Intended Audience :: Developers",
