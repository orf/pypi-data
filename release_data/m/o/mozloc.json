    "1.4.0": {
        "info": {
            "author": "Michael Hirsch, Ph.D.",
            "author_email": "scivision@users.noreply.github.com",
            "bugtrack_url": null,
            "classifiers": [
                "Development Status :: 5 - Production/Stable",
                "Environment :: Console",
                "Intended Audience :: Information Technology",
                "Intended Audience :: System Administrators",
