    "1.3.0": {
        "info": {
            "author": "Yasser Tahiri",
            "author_email": "yasserth19@gmail.com",
            "bugtrack_url": null,
            "classifiers": [
                "Framework :: Django",
                "Intended Audience :: Developers",
                "Intended Audience :: Information Technology",
                "License :: OSI Approved :: MIT License",
                "Natural Language :: English",
