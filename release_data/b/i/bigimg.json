    "0.0.3b": {
        "info": {
            "author": "Stephen Balaban",
            "author_email": "stephen@stephenbalaban.com",
            "bugtrack_url": null,
            "classifiers": [
                "Intended Audience :: Developers",
                "Intended Audience :: Education",
