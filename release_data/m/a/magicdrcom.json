    "1.2.0": {
        "info": {
            "author": "ryuchen",
            "author_email": "chenhaom1993@hotmail.com",
            "bugtrack_url": null,
            "classifiers": [
                "Development Status :: 3 - Alpha",
                "Environment :: Console",
