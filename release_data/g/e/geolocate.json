    "0.1.1": {
        "info": {
            "author": "Gabriel Gazola Milan",
            "author_email": "gabriel.gazola@poli.ufrj.br",
            "bugtrack_url": null,
            "classifiers": [
                "Development Status :: 4 - Beta",
