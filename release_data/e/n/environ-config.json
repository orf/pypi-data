    "22.1.0": {
        "info": {
            "author": "Hynek Schlawack",
            "author_email": "hs@ox.cx",
            "bugtrack_url": null,
            "classifiers": [
                "Development Status :: 5 - Production/Stable",
                "Intended Audience :: Developers",
