    "0.9.3": {
        "info": {
            "author": "Sami Hangaslammi",
            "author_email": "shang@iki.fi",
            "bugtrack_url": null,
            "classifiers": [
                "Development Status :: 3 - Alpha",
                "Environment :: MacOS X",
