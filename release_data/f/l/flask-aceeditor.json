    "1.0.7": {
        "info": {
            "author": "LRS",
            "author_email": "liurongshuo2022@outlook.com",
            "bugtrack_url": null,
            "classifiers": [
                "License :: OSI Approved :: MIT License",
                "Programming Language :: Python",
