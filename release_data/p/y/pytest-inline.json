    "1.0.0": {
        "info": {
            "author": "Yu Liu",
            "author_email": "yuki.liu@utexas.edu",
            "bugtrack_url": null,
            "classifiers": [
                "Development Status :: 4 - Beta",
                "Framework :: Pytest",
                "Intended Audience :: Developers",
                "License :: OSI Approved :: MIT License",
