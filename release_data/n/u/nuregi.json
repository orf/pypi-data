    "0.2.0.dev1": {
        "info": {
            "author": "aldan",
            "author_email": "gitaldan@gmail.com",
            "bugtrack_url": null,
            "classifiers": [
                "Development Status :: 4 - Beta",
                "Intended Audience :: Developers",
