    "0.1.3": {
        "info": {
            "author": "Jisoo Song",
            "author_email": "jeesoo9595@snu.ac.kr",
            "bugtrack_url": null,
            "classifiers": [
                "Development Status :: 4 - Beta",
                "Intended Audience :: Developers",
