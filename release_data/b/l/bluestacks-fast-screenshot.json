    "0.11": {
        "info": {
            "author": "Johannes Fischer",
            "author_email": "<aulasparticularesdealemaosp@gmail.com>",
            "bugtrack_url": null,
            "classifiers": [
                "Development Status :: 4 - Beta",
                "Programming Language :: Python :: 3 :: Only",
