    "0.0.3": {
        "info": {
            "author": "Alvin Shaita",
            "author_email": "alvinshaita@gmail.com",
            "bugtrack_url": null,
            "classifiers": [
                "Intended Audience :: Developers",
                "License :: OSI Approved :: MIT License",
                "Programming Language :: Python :: 2",
