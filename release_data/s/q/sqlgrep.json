    "0.0.2": {
        "info": {
            "author": "Yaroslav Polyakov",
            "author_email": "yaroslaff@gmail.com",
            "bugtrack_url": null,
            "classifiers": [
                "Development Status :: 3 - Alpha",
                "Intended Audience :: Developers",
