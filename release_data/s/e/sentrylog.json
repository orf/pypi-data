    "0.0.3": {
        "info": {
            "author": "Zack",
            "author_email": "1125564921@qq.com",
            "bugtrack_url": null,
            "classifiers": [
                "License :: OSI Approved :: MIT License",
                "Operating System :: OS Independent",
