    "0.0.1": {
        "info": {
            "author": "Martin Kartawijaya",
            "author_email": "pypi@m.kartawijaya.dev",
            "bugtrack_url": null,
            "classifiers": [
                "Development Status :: 3 - Alpha",
                "License :: OSI Approved :: BSD License",
