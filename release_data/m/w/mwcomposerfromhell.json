    "0.4": {
        "info": {
            "author": "Patrick Cloke",
            "author_email": "clokep@patrick.cloke.us",
            "bugtrack_url": null,
            "classifiers": [
                "Development Status :: 3 - Alpha",
                "Intended Audience :: Developers",
