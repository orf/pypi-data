    "0.3.3": {
        "info": {
            "author": "Iztok Fister Jr.",
            "author_email": "iztok@iztok-jr-fister.eu",
            "bugtrack_url": null,
            "classifiers": [
                "License :: OSI Approved :: MIT License",
                "Programming Language :: Python :: 3",
