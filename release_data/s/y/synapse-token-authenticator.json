    "0.2.0": {
        "info": {
            "author": null,
            "author_email": "Sorunome <mail@sorunome.de>, Amanda Graven <amanda@graven.dev>, Jan Christian Gr\u00fcnhage <jan.christian@gruenhage.xyz>",
            "bugtrack_url": null,
            "classifiers": [
                "Development Status :: 4 - Beta",
                "Programming Language :: Python",
                "Programming Language :: Python :: 3.10",
                "Programming Language :: Python :: 3.11",
