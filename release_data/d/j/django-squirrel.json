    "0.2.9": {
        "info": {
            "author": "Jose Salgado",
            "author_email": "jose.salgado.wrk@gmail.com",
            "bugtrack_url": null,
            "classifiers": [
                "Environment :: Web Environment",
                "Framework :: Django",
                "Framework :: Django :: 2.2",
                "Intended Audience :: Developers",
                "License :: OSI Approved :: BSD License",
                "Operating System :: OS Independent",
