    "0.1.0": {
        "info": {
            "author": "",
            "author_email": "Alexis Jeandet <alexis.jeandet@member.fsf.org>",
            "bugtrack_url": null,
            "classifiers": [
                "Development Status :: 4 - Beta",
                "Intended Audience :: Science/Research",
                "License :: OSI Approved :: GNU General Public License v3 (GPLv3)",
                "Natural Language :: English",
