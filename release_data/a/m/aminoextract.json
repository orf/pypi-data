    "0.2.0": {
        "info": {
            "author": "Florian Zwagemaker",
            "author_email": "ids-bioinformatics@rivm.nl",
            "bugtrack_url": null,
            "classifiers": [
                "Development Status :: 4 - Beta",
                "Intended Audience :: Science/Research",
