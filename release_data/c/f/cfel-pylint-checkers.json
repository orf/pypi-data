    "1.0.2": {
        "info": {
            "author": "Philipp Middendorf",
            "author_email": "philipp.middendorf@desy.de",
            "bugtrack_url": null,
            "classifiers": [
                "License :: OSI Approved :: GNU General Public License v3 or later (GPLv3+)",
