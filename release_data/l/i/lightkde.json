    "1.0.2": {
        "info": {
            "author": "Arpad Rozsas",
            "author_email": "rozsasarpi@gmail.com",
            "bugtrack_url": null,
            "classifiers": [
                "Intended Audience :: Science/Research",
                "Operating System :: OS Independent",
