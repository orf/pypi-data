    "0.1.2": {
        "info": {
            "author": "Tobi-De",
            "author_email": "tobidegnon@protonmail.com",
            "bugtrack_url": null,
            "classifiers": [
                "Development Status :: 4 - Beta",
                "Environment :: Console",
