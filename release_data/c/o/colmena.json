    "0.3.4": {
        "info": {
            "author": "Globus Labs",
            "author_email": "labs@globus.org",
            "bugtrack_url": null,
            "classifiers": [
                "Development Status :: 3 - Alpha",
                "Intended Audience :: Science/Research",
