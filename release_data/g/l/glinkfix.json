    "1.0.7": {
        "info": {
            "author": "Peter Nardi",
            "author_email": "pete@nardi.com",
            "bugtrack_url": null,
            "classifiers": [
                "Development Status :: 5 - Production/Stable",
                "Intended Audience :: Developers",
