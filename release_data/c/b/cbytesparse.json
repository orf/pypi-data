    "0.0.5": {
        "info": {
            "author": "Andrea Zoppi",
            "author_email": "texzk@email.it",
            "bugtrack_url": null,
            "classifiers": [
                "Development Status :: 2 - Pre-Alpha",
                "Intended Audience :: Developers",
