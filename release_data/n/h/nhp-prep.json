    "0.1.3": {
        "info": {
            "author": "Hugo Angulo, Zijun Zhao",
            "author_email": "hugoanda@andrew.cmu.edu, zijunzha@andrew.cmu.edu",
            "bugtrack_url": null,
            "classifiers": [
                "Development Status :: 5 - Production/Stable",
