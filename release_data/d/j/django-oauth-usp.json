    "1.1.0": {
        "info": {
            "author": "Marcelo Schneider",
            "author_email": "schneider.fei@gmail.com",
            "bugtrack_url": null,
            "classifiers": [
                "Environment :: Web Environment",
                "Framework :: Django",
