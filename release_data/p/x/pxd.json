    "0.3.0": {
        "info": {
            "author": "polardbx",
            "author_email": "vettal.wd@alibaba-inc.com",
            "bugtrack_url": null,
            "classifiers": [
                "License :: OSI Approved :: Apache Software License",
                "Operating System :: OS Independent",
                "Programming Language :: Python",
                "Programming Language :: Python :: 3",
