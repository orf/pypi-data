    "0.1.0": {
        "info": {
            "author": "QuantCo, Inc.",
            "author_email": "",
            "bugtrack_url": null,
            "classifiers": [
                "Development Status :: 3 - Alpha",
                "Intended Audience :: Developers",
                "Intended Audience :: Science/Research",
                "License :: OSI Approved :: BSD License",
                "Programming Language :: Python :: 3",
