        "info": {
            "author": "Matthew Bonig<matthew.bonig@gmail.com>",
            "author_email": "",
            "bugtrack_url": null,
            "classifiers": [
                "Development Status :: 5 - Production/Stable",
                "Intended Audience :: Developers",
