    "0.1.3": {
        "info": {
            "author": "Charlie Denton",
            "author_email": "charlie@meshy.co.uk",
            "bugtrack_url": null,
            "classifiers": [
                "Intended Audience :: Developers",
                "License :: OSI Approved :: Apache Software License",
