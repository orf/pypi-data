    "0.2.0": {
        "info": {
            "author": "Akshay Raj Gollahalli",
            "author_email": "akshay@gollahalli.com",
            "bugtrack_url": null,
            "classifiers": [
                "Development Status :: 3 - Alpha",
                "Environment :: Console",
