    "1.3": {
        "info": {
            "author": "Athrv Chaulkar",
            "author_email": "athrvchaulkar@gmail.com",
            "bugtrack_url": null,
            "classifiers": [
                "Development Status :: 5 - Production/Stable",
                "Environment :: Console",
                "License :: OSI Approved :: MIT License",
                "Operating System :: Microsoft :: Windows",
