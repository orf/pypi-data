    "0.2.2": {
        "info": {
            "author": "Coral Sistemas",
            "author_email": "",
            "bugtrack_url": null,
            "classifiers": [
                "Environment :: Web Environment",
                "Framework :: Django",
                "Intended Audience :: Developers",
