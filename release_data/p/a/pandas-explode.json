    "0.0.6": {
        "info": {
            "author": "Oren Ovadia",
            "author_email": "orenovad@gmail.com",
            "bugtrack_url": null,
            "classifiers": [
                "Development Status :: 3 - Alpha",
                "Intended Audience :: Developers",
