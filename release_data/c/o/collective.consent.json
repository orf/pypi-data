    "1.0b3": {
        "info": {
            "author": "Maik Derstappen",
            "author_email": "md@derico.de",
            "bugtrack_url": null,
            "classifiers": [
                "Environment :: Web Environment",
                "Framework :: Plone",
                "Framework :: Plone :: 4.3",
                "Framework :: Plone :: 5.0",
                "Framework :: Plone :: 5.1",
                "Framework :: Plone :: 5.2",
