    "0.1.3": {
        "info": {
            "author": "Dmitry Pershin",
            "author_email": "dapper1291@gmail.com",
            "bugtrack_url": null,
            "classifiers": [
                "Framework :: AsyncIO",
                "Intended Audience :: Developers",
