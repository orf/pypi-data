    "1.1.0": {
        "info": {
            "author": "Steven Loria",
            "author_email": "sloria1@gmail.com",
            "bugtrack_url": null,
            "classifiers": [
                "License :: OSI Approved :: MIT License",
                "Programming Language :: Python :: 3",
