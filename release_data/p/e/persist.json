    "3.1": {
        "info": {
            "author": "Michael McNeil Forbes",
            "author_email": "michael.forbes+python@gmail.com",
            "bugtrack_url": null,
            "classifiers": [
                "Development Status :: 4 - Beta",
                "Intended Audience :: Developers",
