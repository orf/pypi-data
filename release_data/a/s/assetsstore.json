    "1.0.3": {
        "info": {
            "author": "Nebojsa Mrkic",
            "author_email": "mrkic.nebojsa@gmail.com",
            "bugtrack_url": null,
            "classifiers": [
                "Environment :: Web Environment",
                "Intended Audience :: Developers",
