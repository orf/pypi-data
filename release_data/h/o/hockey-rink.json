    "0.1.8": {
        "info": {
            "author": "The Bucketless",
            "author_email": "thebucketless@protonmail.com",
            "bugtrack_url": null,
            "classifiers": [
                "Development Status :: 3 - Alpha",
                "Framework :: Matplotlib",
                "License :: OSI Approved :: GNU General Public License v3 (GPLv3)",
                "Programming Language :: Python :: 3.6"
