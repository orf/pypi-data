    "0.0.7": {
        "info": {
            "author": "Mepla",
            "author_email": "soheil.nasirian@gmail.com",
            "bugtrack_url": null,
            "classifiers": [
                "Development Status :: 2 - Pre-Alpha",
                "Intended Audience :: Developers",
