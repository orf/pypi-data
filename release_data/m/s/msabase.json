    "0.0.5": {
        "info": {
            "author": "Stefan Welcker",
            "author_email": "stefan@u2d.ai",
            "bugtrack_url": null,
            "classifiers": [
                "Development Status :: 4 - Beta",
                "Framework :: FastAPI",
